      "type": "enumeration",
      "enum": [
        "none",
        "white",
        "gray",
        "signature",
        "black",
        "blue",
        "frostBlue",
        "oceanBlue",
        "peach",
        "violet",
        "gradientRadialTopBlueSignature",
        "gradientRadialCenterBlueSignature",
        "gradientLinearBlueSignature"
