    "effect": {
      "type": "enumeration",
      "enum": [
        "coverflow",
        "slide"
      ],
      "default": "slide"
    },
    "carouselWidth": {
      "type": "enumeration",
      "enum": [
        "fluid",
        "xxWide",
        "xWide"
      ],
