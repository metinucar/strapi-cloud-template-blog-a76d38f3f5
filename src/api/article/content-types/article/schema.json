{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": "Blog posts and articles"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "fullSlug": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "site": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "enum": [
        "main",
        "investor"
      ]
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "category": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "featuredImage": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "author": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "publishedDate": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "readTime": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "body": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "content.editorial",
        "content.editorial-markdown",
        "content.editorial-image",
        "content.editorial-image-keep-proportions",
        "content.editorial-quote",
        "cta.cta-block"
      ]
    },
    "contentGrid": {
      "type": "component",
      "component": "grids.content-grid-slider",
      "repeatable": false
    },
    "ctaForm": {
      "type": "component",
      "component": "cta.cta-form",
      "repeatable": false
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author"
    },
    "collaborators": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author"
    },
    "productCategoryTags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-category-tag.product-category-tag"
    },
    "solutionTags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::solution-tag.solution-tag"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag"
    },
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo"
    },
    "hideBreadcrumbs": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": false
    },
    "hideStickyMenu": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": false
    },
    "storyblokUuid": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
